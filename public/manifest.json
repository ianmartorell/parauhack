{
  "name": "Parauhack",
  "description": "Afegeix un botó al Paraulògic per introduir automàticament la solució d'avui.",
  "version": "0.1.0",
  "manifest_version": 3,
  "action": {},
  "content_scripts": [
    {
      "matches": [
        "https://vilaweb.cat/paraulogic/*"
